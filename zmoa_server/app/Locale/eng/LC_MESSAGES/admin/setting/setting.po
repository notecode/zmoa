msgid "help_geocode"
msgstr "Please enter your store location geocode manually."

msgid "help_open"
msgstr "Fill in your store\'s opening times."

msgid "help_comment"
msgstr "This field is for any special notes you would like to tell the customer i.e. Store does not accept cheques."

msgid "help_location"
msgstr "The different store locations you have that you want displayed on the contact us form."

msgid "help_currency"
msgstr "Change the default currency. Clear your browser cache to see the change and reset your existing cookie."

msgid "help_currency_auto"
msgstr "Set your store to automatically update currencies daily."

msgid "help_limit_admin"
msgstr "Determines how many admin items are shown per page (orders, customers, etc)."

msgid "help_product_count"
msgstr "Show the number of products inside the subcategories in the storefront header category menu. Be warned, this will cause an extreme performance hit for stores with a lot of subcategories!"

msgid "help_review"
msgstr "Enable/Disable new review entry and display of existing reviews."

msgid "help_review_guest"
msgstr "Allow guests to post reviews."

msgid "help_voucher_min"
msgstr "Minimum amount a customer can purchase a voucher for."

msgid "help_voucher_max"
msgstr "Maximum amount a customer can purchase a voucher for."

msgid "help_tax_default"
msgstr "Use the store address to calculate taxes if customer is not logged in. You can choose to use the store address for the customer\'s shipping or payment address."

msgid "help_tax_customer"
msgstr "Use the customer\'s default address when they login to calculate taxes. You can choose to use the default address for the customer\'s shipping or payment address."

msgid "help_customer_online"
msgstr "Track customers online via the customer reports section."

msgid "help_customer_activity"
msgstr "Track customers activity via the customer reports section."

msgid "help_customer_group"
msgstr "Default customer group."

msgid "help_customer_group_display"
msgstr "Display customer groups that new customers can select to use such as wholesale and business when signing up."

msgid "help_customer_price"
msgstr "Only show prices when a customer is logged in."

msgid "help_login_attempts"
msgstr "Maximum login attempts allowed before the account is locked for 1 hour. Customer and affliate accounts can be unlocked on the customer or affliate admin pages."

msgid "help_account"
msgstr "Forces people to agree to terms before an account can be created."

msgid "help_invoice_prefix"
msgstr "Set the invoice prefix (e.g. INV-2011-00). Invoice IDs will start at 1 for each unique prefix."

msgid "help_cart_weight"
msgstr "Show the cart weight on the cart page."

msgid "help_checkout_guest"
msgstr "Allow customers to checkout without creating an account. This will not be available when a downloadable product is in the shopping cart."

msgid "help_checkout"
msgstr "Forces people to agree to terms before a customer can checkout."

msgid "help_order_status"
msgstr "Set the default order status when an order is processed."

msgid "help_processing_status"
msgstr "Set the order status the customer\'s order must reach before the order starts stock subtraction and coupon, voucher and rewards redemption."

msgid "help_complete_status"
msgstr "Set the order status the customer\'s order must reach before they are allowed to access their downloadable products and gift vouchers."

msgid "help_fraud_status"
msgstr "Set the order status when a customer is suspected of trying to alter the order payment details or use a coupon, gift voucher or reward points that have already been used."

msgid "help_api"
msgstr "Default API user the admin should use."

msgid "help_stock_display"
msgstr "Display stock quantity on the product page."

msgid "help_stock_warning"
msgstr "Display out of stock message on the shopping cart page if a product is out of stock but stock checkout is yes. (Warning always shows if stock checkout is no)"

msgid "help_stock_checkout"
msgstr "Allow customers to still checkout if the products they are ordering are not in stock."

msgid "help_affiliate_approval"
msgstr "Automatically approve any new affiliates who sign up."

msgid "help_affiliate_auto"
msgstr "Automatically add commission when each order reaches the complete status."

msgid "help_affiliate_commission"
msgstr "The default affiliate commission percentage."

msgid "help_affiliate"
msgstr "Forces people to agree to terms before an affiliate account can be created."

msgid "help_return"
msgstr "Forces people to agree to terms before a return can be created."

msgid "help_return_status"
msgstr "Set the default return status when a return request is submitted."

msgid "help_captcha"
msgstr "Captcha to use for registration, login, contact and reviews."

msgid "help_icon"
msgstr "The icon should be a PNG that is 16px x 16px."

msgid "help_ftp_root"
msgstr "The directory your OpenCart installation is stored in. Normally \'public_html/\'."

msgid "help_mail_protocol"
msgstr "Only choose \'Mail\' unless your host has disabled the php mail function."

msgid "help_mail_parameter"
msgstr "When using \'Mail\', additional mail parameters can be added here (e.g. -f email@storeaddress.com)."

msgid "help_mail_smtp_hostname"
msgstr "Add \'tls://\' or \'ssl://\' prefix if security connection is required. (e.g. tls://smtp.gmail.com, ssl://smtp.gmail.com)."

msgid "help_mail_smtp_password"
msgstr "For gmail you might need to setup a application specific password here: https://security.google.com/settings/security/apppasswords."

msgid "help_mail_alert"
msgstr "Select which features you would like to receive an alert email on when a customer uses them."

msgid "help_mail_alert_email"
msgstr "Any additional emails you want to receive the alert email, in addition to the main store email. (comma separated)."

msgid "help_secure"
msgstr "To use SSL check with your host if a SSL certificate is installed and add the SSL URL to the catalog and admin config files."

msgid "help_shared"
msgstr "Try to share the session cookie between stores so the cart can be passed between different domains."

msgid "help_robots"
msgstr "A list of web crawler user agents that shared sessions will not be used with. Use separate lines for each user agent."

msgid "help_seo_url"
msgstr "To use SEO URLs, apache module mod-rewrite must be installed and you need to rename the htaccess.txt to .htaccess."

msgid "help_file_max_size"
msgstr "The maximum image file size you can upload in Image Manager. Enter as byte."

msgid "help_file_ext_allowed"
msgstr "Add which file extensions are allowed to be uploaded. Use a new line for each value."

msgid "help_file_mime_allowed"
msgstr "Add which file mime types are allowed to be uploaded. Use a new line for each value."

msgid "help_maintenance"
msgstr "Prevents customers from browsing your store. They will instead see a maintenance message. If logged in as admin, you will see the store as normal."

msgid "help_password"
msgstr "Allow forgotten password to be used for the admin. This will be disabled automatically if the system detects a hack attempt."

msgid "help_encryption"
msgstr "Please provide a secret key that will be used to encrypt private information when processing orders."

msgid "help_compression"
msgstr "GZIP for more efficient transfer to requesting clients. Compression level must be between 0 - 9."

msgid "error_warning"
msgstr "Warning: Please check the form carefully for errors!"

msgid "error_permission"
msgstr "Warning: You do not have permission to modify settings!"

msgid "error_meta_title"
msgstr "Title must be between 3 and 32 characters!"

msgid "error_name"
msgstr "Store Name must be between 3 and 32 characters!"

msgid "error_owner"
msgstr "Store Owner must be between 3 and 64 characters!"

msgid "error_address"
msgstr "Store Address must be between 10 and 256 characters!"

msgid "error_email"
msgstr "E-Mail Address does not appear to be valid!"

msgid "error_telephone"
msgstr "Telephone must be between 3 and 32 characters!"

msgid "error_limit"
msgstr "Limit required!"

msgid "error_login_attempts"
msgstr "Login Attempts must be greater than 0!"

msgid "error_customer_group_display"
msgstr "You must include the default customer group if you are going to use this feature!"

msgid "error_voucher_min"
msgstr "Minimum voucher amount required!"

msgid "error_voucher_max"
msgstr "Maximum voucher amount required!"

msgid "error_processing_status"
msgstr "You must choose at least 1 order process status"

msgid "error_complete_status"
msgstr "You must choose at least 1 order complete status"

msgid "error_ftp_hostname"
msgstr "FTP Host required!"

msgid "error_ftp_port"
msgstr "FTP Port required!"

msgid "error_ftp_username"
msgstr "FTP Username required!"

msgid "error_ftp_password"
msgstr "FTP Password required!"

msgid "error_error_filename"
msgstr "Error Log Filename required!"

msgid "error_malformed_filename"
msgstr "Error Malformed Log Filename!"

msgid "error_encryption"
msgstr "Encryption Key must be between 32 and 1024 characters!"
